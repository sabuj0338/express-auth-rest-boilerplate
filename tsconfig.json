{
    "compilerOptions": {
        "outDir": "./build",
        "rootDir": "./src",
        "sourceMap": true,
        "target": "esnext",
        "module": "commonjs",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "noImplicitAny": true,
        "strict": true,
        "skipLibCheck": true
    },
    "include": [
        "src/**/*"
    ],
    "files": ["./src/custom.d.ts"]
}